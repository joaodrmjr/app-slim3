{% extends 'templates/app.twig' %}


{% block content %}
<div>
	<form>
		<h1>Login</h1>


		<label>Username</label>
		<div>
			<input type="text" name="username">
		</div>

		<label>Senha</label>
		<div>
			<input type="password" name="password">
		</div>


		<div>
			<input type="submit" value="Logar">
		</div>
	</form>
</div>
{% endblock %}